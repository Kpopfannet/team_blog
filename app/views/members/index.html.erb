<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Subname</th>
      <th>Img</th>
      <th>Desc</th>
      <th>Favorite</th>
      <th>Hate</th>
      <th>Wannabe</th>
      <th>Birthday</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.subname %></td>
        <td><%= member.img %></td>
        <td><%= member.desc %></td>
        <td><%= member.favorite %></td>
        <td><%= member.hate %></td>
        <td><%= member.wannabe %></td>
        <td><%= member.birthday %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
